{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-new-yeruva"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"ls_to_azure_sql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_to_azure_sql'"
		},
		"ls_to_blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_to_blob'"
		},
		"ls_to_onPrem_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_to_onPrem'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://yeruvadatalake.dfs.core.windows.net/"
		},
		"ls_to_http_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://upload.wikimedia.org"
		},
		"new-selfhosted_properties_typeProperties_linkedInfo_resourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/f13f4ae7-b396-4d5b-8073-87e9cabe685e/resourcegroups/practice1/providers/Microsoft.DataFactory/factories/adf-practice-yeruva/integrationruntimes/integrationRuntime1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-new-yeruva_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"ls_to_azure_sql_connectionString": {
						"value": "[parameters('ls_to_azure_sql_connectionString')]"
					},
					"ls_to_blob_connectionString": {
						"value": "[parameters('ls_to_blob_connectionString')]"
					},
					"ls_to_onPrem_connectionString": {
						"value": "[parameters('ls_to_onPrem_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"ls_to_http_properties_typeProperties_url": {
						"value": "[parameters('ls_to_http_properties_typeProperties_url')]"
					},
					"new-selfhosted_properties_typeProperties_linkedInfo_resourceId": {
						"value": "[parameters('new-selfhosted_properties_typeProperties_linkedInfo_resourceId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}